<p id="notice"><%= notice %></p>

<h1>Gene Names</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gene_names.each do |gene_name| %>
      <tr>
        <td><%= link_to 'Show', gene_name %></td>
        <td><%= link_to 'Edit', edit_gene_name_path(gene_name) %></td>
        <td><%= link_to 'Destroy', gene_name, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gene Name', new_gene_name_path %>
